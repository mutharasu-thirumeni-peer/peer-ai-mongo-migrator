{
  "name": "peer-ai-mongo-migrator",
  "version": "2.0.0",
  "description": "Enhanced TypeScript-based PeerAI agent for MongoDB migration operations, schema management, real-time monitoring, advanced database orchestration, and comprehensive ER diagram generation",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "interactive": "tsx src/index.ts interactive",
    "test": "jest",
    "test:mcp": "jest --testNamePattern='MCP'",
    "test:cross-db": "jest --testNamePattern='CrossDatabase'",
    "test:schema": "jest --testNamePattern='Schema'",
    "test:migration": "jest --testNamePattern='Migration'",
    "test:er-diagram": "node test-er-diagram.js",
    "test:smart-patterns": "node test-smart-patterns.js",
    "test:advanced-patterns": "node test-advanced-patterns.js",
    "debug": "node debug-connection.js",
    "test:mongodb": "node test-mongodb-mcp.js",
    "test:html-viewer": "npx tsx test-html-viewer.js",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "setup": "./scripts/setup-env.sh",
    "setup:env": "./scripts/setup-env.sh"
  },
  "keywords": [
    "peerai",
    "mongodb",
    "migration",
    "agent",
    "database",
    "orchestration",
    "schema-management",
    "real-time-monitoring",
    "data-migration",
    "health-checks"
  ],
  "author": "PeerAI",
  "license": "MIT",
  "dependencies": {
    "@mermaid-js/mermaid-cli": "^11.9.0",
    "@modelcontextprotocol/sdk": "^0.4.0",
    "@octokit/rest": "^20.0.1",
    "axios": "^1.11.0",
    "chalk": "^5.3.0",
    "cheerio": "^1.1.2",
    "commander": "^11.1.0",
    "compromise": "^14.14.4",
    "didyoumean2": "^7.0.4",
    "dotenv": "^16.3.1",
    "fastest-levenshtein": "^1.0.16",
    "fs-extra": "^11.1.1",
    "fuse.js": "^7.1.0",
    "inquirer": "^9.2.12",
    "mermaid": "^11.10.1",
    "mongodb": "^6.3.0",
    "natural": "^8.1.0",
    "openai": "^4.104.0",
    "ora": "^7.0.1",
    "pg": "^8.11.3",
    "simple-git": "^3.20.0",
    "temp": "^0.9.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
